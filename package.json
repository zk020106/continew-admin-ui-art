{
  "name": "art-design-pro",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=8.8.0"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit",
    "serve": "vite preview",
    "lint": "eslint",
    "fix": "eslint --fix",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint  \"**/*.{css,scss,vue}\" --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky",
    "commit": "git-cz",
    "clean:dev": "tsx scripts/clean-dev.ts"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts,mjs,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json,jsonc}": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix --allow-empty-input",
      "prettier --write"
    ],
    "*.{html,htm}": [
      "prettier --write"
    ],
    "*.{scss,css,less}": [
      "stylelint --fix --allow-empty-input",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ],
    "*.{yaml,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@vue-office/docx": "^1.6.3",
    "@vue-office/excel": "^1.7.14",
    "@vue-office/pdf": "^2.0.10",
    "@vue/reactivity": "^3.5.26",
    "@vueuse/core": "^13.9.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "next",
    "axios": "^1.13.2",
    "cron-parser": "^5.4.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "element-plus": "^2.13.1",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.11.1",
    "jsencrypt": "^3.5.4",
    "lodash-es": "^4.17.22",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "ohash": "^2.0.11",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "qrcode.vue": "^3.6.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26",
    "vue-draggable-plus": "^0.6.1",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4",
    "vue3-tree-org": "^4.2.2",
    "xe-utils": "^3.8.4",
    "xgplayer": "^3.0.23",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.39.2",
    "@iconify-json/ep": "^1.2.3",
    "@iconify-json/fluent": "^1.2.36",
    "@iconify-json/iconamoon": "^1.2.2",
    "@iconify-json/ri": "^1.2.7",
    "@iconify-json/vaadin": "^1.2.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/compiler-sfc": "^3.5.26",
    "baseline-browser-mapping": "^2.9.14",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.97.2",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^36.0.1",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-vue-components": "^29.2.0",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.7.9",
    "vue-demi": "^0.14.10",
    "vue-img-cutter": "^3.0.7",
    "vue-tsc": "~2.1.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vue-office/docx",
      "@vue-office/excel",
      "@vue-office/pdf",
      "vue-demi"
    ],
    "overrides": {
      "tar@=7.5.1": ">=7.5.2",
      "tmp@<=0.2.3": ">=0.2.4",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "preact@>=10.27.0 <10.27.3": ">=10.27.3"
    }
  }
}
